<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ztf.controller.ChatListController">
   <children>
      <VBox prefHeight="700.0" prefWidth="300.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: ffcc33;">
               <children>
                  <ImageView fx:id="iv_head" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                     <HBox.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </HBox.margin>
                  </ImageView>
                  <VBox prefHeight="120.0" prefWidth="395.0">
                     <children>
                        <Label fx:id="lb_name" prefHeight="20.0" prefWidth="243.0" text="Label">
                           <font>
                              <Font name="System Bold" size="25.0" />
                           </font>
                        </Label>
                        <Label fx:id="lb_ip" prefHeight="20.0" prefWidth="268.0" text="Label">
                           <VBox.margin>
                              <Insets top="5.0" />
                           </VBox.margin>
                        </Label>
                        <Label fx:id="lb_port" prefHeight="20.0" prefWidth="207.0" text="Label">
                           <VBox.margin>
                              <Insets top="5.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                     <HBox.margin>
                        <Insets left="10.0" top="20.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0" />
               </children>
            </HBox>
            <Label prefHeight="32.0" prefWidth="326.0" style="-fx-background-color: FFEC8B;" text="在线好友" textFill="#250303">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ListView fx:id="lv_chatList" prefHeight="472.0" prefWidth="300.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
